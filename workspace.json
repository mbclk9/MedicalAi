{
  "version": 2,
  "projects": {
    "frontend": {
      "root": "apps/frontend",
      "projectType": "application",
      "sourceRoot": "apps/frontend/src",
      "targets": {
        "build": {
          "executor": "@nx/vite:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "apps/frontend/dist"
          }
        },
        "serve": {
          "executor": "@nx/vite:dev-server",
          "options": {
            "buildTarget": "frontend:build"
          }
        }
      }
    },
    "backend": {
      "root": "apps/backend",
      "projectType": "application",
      "sourceRoot": "apps/backend/src",
      "targets": {
        "build": {
          "executor": "@nx/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "apps/backend/dist",
            "main": "apps/backend/src/index.ts",
            "tsConfig": "apps/backend/tsconfig.json"
          }
        },
        "serve": {
          "executor": "@nx/node:execute",
          "options": {
            "buildTarget": "backend:build"
          }
        }
      }
    },
    "types": {
      "root": "packages/types",
      "projectType": "library",
      "sourceRoot": "packages/types",
      "targets": {
        "build": {
          "executor": "@nx/js:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "packages/types/dist",
            "main": "packages/types/index.ts",
            "tsConfig": "packages/types/tsconfig.json"
          }
        }
      }
    }
  }
}